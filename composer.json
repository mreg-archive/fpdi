{
    "name": "itbz/fpdi",
    "description": "Unofficial PSR-4 compliant version of the FPDI library",
    "type": "library",
    "license": "Apache-2.0",
    "require": {
        "php": ">=5.3.0",
        "itbz/fpdf": "1.7.*"
    },
    "require-dev": {
        "tecnick.com/tcpdf": "6.*",
        "phpunit/phpunit": "4.*",
        "hanneskod/classtools": "1.0.0-beta3",
        "symfony/finder": "~2.3",
        "nikic/php-parser": "1.0.0-beta1"
    },
    "suggest": {
        "tecnick.com/tcpdf": "FPDI supports TCPDF as a replacement for FPDF"
    },
    "autoload": {
        "psr-4": {
            "fpdi\\": "src/",
            "fpdidev\\": "dev/"
        }
    }
}
